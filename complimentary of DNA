DNA = input("Enter the DNA sequence here : ") 
print('\n') 
print("The Entered DNA sequence is :", DNA) 
t = ord("t") 
a = DNA.replace("a", str(t)) 
a = a.replace("t", "a") 
a = a.replace(str(t), "t") 
g = ord("g") 
a = a.replace("g", str(g)) 
a = a.replace("c", "g") 
a = a.replace(str(g), "c") 
print("Complementary DNA Sequence :", a) 
